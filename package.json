{
  "name": "hast-util-to-mdast",
  "version": "9.0.0",
  "description": "hast utility to transform to mdast",
  "license": "MIT",
  "keywords": [
    "unist",
    "hast",
    "hast-util",
    "mdast",
    "mdast-util",
    "rehype",
    "remark",
    "markdown",
    "html",
    "transform"
  ],
  "repository": "syntax-tree/hast-util-to-mdast",
  "bugs": "https://github.com/syntax-tree/hast-util-to-mdast/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Seth Vincent <sethvincent@gmail.com> (https://sethvincent.com)",
  "contributors": [
    "Seth Vincent <sethvincent@gmail.com> (https://sethvincent.com)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/extend": "^3.0.0",
    "@types/hast": "^3.0.0",
    "@types/mdast": "^4.0.0",
    "@types/unist": "^3.0.0",
    "extend": "^3.0.0",
    "hast-util-phrasing": "^3.0.0",
    "hast-util-to-html": "^9.0.0",
    "hast-util-to-text": "^4.0.0",
    "hast-util-whitespace": "^3.0.0",
    "mdast-util-phrasing": "^4.0.0",
    "mdast-util-to-hast": "^13.0.0",
    "mdast-util-to-string": "^4.0.0",
    "rehype-minify-whitespace": "^5.0.0",
    "trim-trailing-lines": "^2.0.0",
    "unist-util-position": "^5.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "c8": "^8.0.0",
    "hast-util-from-html": "^2.0.0",
    "hastscript": "^8.0.0",
    "is-hidden": "^2.0.0",
    "mdast-util-assert": "^5.0.0",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-to-markdown": "^2.0.0",
    "micromark-extension-gfm": "^3.0.0",
    "prettier": "^3.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^5.0.0",
    "unist-builder": "^4.0.0",
    "unist-util-remove-position": "^5.0.0",
    "xo": "^0.55.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . -qfo && prettier . -w --log-level warn && xo --fix",
    "test-api": "node --conditions development test/index.js",
    "test-coverage": "c8 --check-coverage --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "max-depth": "off",
      "unicorn/prefer-at": "off",
      "unicorn/prefer-string-replace-all": "off"
    },
    "overrides": [
      {
        "files": "test/**/*.js",
        "rules": {
          "no-await-in-loop": 0
        }
      }
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm",
      [
        "remark-lint-no-html",
        false
      ]
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreFiles": [
      "lib/types.d.ts"
    ]
  }
}
